{
  "extends": [
    "local>brave/renovate-config"
  ],
  "packageRules": [
    {
      "matchPackageNames": [
        "@aws-sdk",
        "@aws-sdk/client-dynamodb",
        "@aws-sdk/client-s3",
        "@sentry/node"
      ],
      "matchUpdateTypes": [
        "patch",
        "minor"
      ],
      "autoApprove": true,
      "automerge": true
    }
  ]
}
